    "format": "biome format --write .",
    "typecheck": "tsc-files --noEmit"
  },
  "dependencies": {
    "@eda/logger": "workspace:*",
    "@eda/types": "workspace:*",
    "@supabase/supabase-js": "^2.45.4",
    "@elysiajs/swagger": "^0.8.0",
    "elysia": "^0.8.17",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  }
}
